X_train, y_train = load_svmlight_file(arq_test)
X_test, y_test = load_svmlight_file(arq_train)
size = X_train.shape
batchsize = 1000
ini = 0
end = batchsize
history = []
while(end <= size[0] ):
xt = X_train[0:end]
yt = y_train[0:end]
print ("Training size... ", end)
start_time = time.time()
main(xt, yt, X_test, y_test, history)
print("--- %s seconds ---" % (time.time() - start_time))
end = end + batchsize
print (history)
quit
